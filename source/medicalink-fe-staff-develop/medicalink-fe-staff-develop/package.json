{
  "name": "medicalink-staff-portal",
  "author": "",
  "description": "Medical Link Staff Portal - Dashboard for Doctors, Staffs, and Super Admins",
  "keywords": [
    "medical",
    "healthcare",
    "staff-portal",
    "dashboard",
    "vite",
    "react",
    "frontend"
  ],
  "license": "MIT",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "setup": "git init && npx husky init",
    "format": "prettier \"src/**/*.{ts,tsx,js,jsx}\" --write",
    "format:check": "prettier \"src/**/*.{ts,tsx,js,jsx}\" --check",
    "lint": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --max-warnings 0 --fix",
    "type-check": "tsc --noEmit",
    "knip": "knip",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "commitlint": "commitlint --edit",
    "commitizen": "exec < /dev/tty && git cz --hook || true",
    "prepare": "husky",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:codegen": "playwright codegen http://localhost:5173",
    "test:report": "playwright show-report",
    "test:settings": "playwright test tests/settings",
    "test:specialties": "playwright test tests/specialties",
    "test:chromium": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:webkit": "playwright test --project=webkit"
  },
  "lint-staged": {
    "src/**/*.{ts,js,html,css,scss}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
